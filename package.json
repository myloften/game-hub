{
  "name": "game-site",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.18.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf .next && rm -rf .output && NEXT_TELEMETRY_DISABLED=1 NODE_ENV=production npm install --legacy-peer-deps && npm install -D tailwindcss@latest postcss@latest autoprefixer@latest && npm install && npx prisma generate && next build && rm -rf .next && find .output -type f -name '*.pack' -delete && find .output -type f -name '*.map' -delete && rm -rf .output/cache",
    "start": "next start",
    "lint": "next lint",
    "seed": "npx tsx prisma/seed.ts",
    "download-images": "npx tsx scripts/download-images.ts"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.8.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@types/axios": "^0.9.36",
    "@types/bcryptjs": "^2.4.6",
    "axios": "^1.8.3",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.358.0",
    "next": "14.1.0",
    "next-auth": "^4.24.6",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.50.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5"
  }
}
